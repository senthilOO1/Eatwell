<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hi_focus.androiduser2.deva.com.eatwell">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/eatwallicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/eatwallicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="hi_focus.androiduser2.deva.com.eatwell.accountDetails.DashBoard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="hi_focus.androiduser2.deva.com.eatwell.accountDetails.LoginActivity" />
        <activity android:name="hi_focus.androiduser2.deva.com.eatwell.accountDetails.OtpScreen" />
        <activity android:name="hi_focus.androiduser2.deva.com.eatwell.accountDetails.SignActivity" />
        <activity android:name="hi_focus.androiduser2.deva.com.eatwell.Home" />
        <activity android:name="hi_focus.androiduser2.deva.com.eatwell.accountDetails.ForgotPassword"></activity>

        <service android:name="hi_focus.androiduser2.deva.com.eatwell.firebase.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name="hi_focus.androiduser2.deva.com.eatwell.firebase.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>